{
	"name": "playwright-ts",
	"version": "18",
	"description": "playwright project with typescript and azure devops integration",
	"main": "index.js",
	"scripts": {
		"pw:browser": "npx playwright install chromium firefox webkit",
		"plan": "npx playwright test --project=chromium --list",
		"test": "npx playwright test --project=chromium --reporter=list",
		"debug": "npx playwright test --project=chromium --headed --reporter=list",
		"inspect": "npx playwright test --project=chromium --debug --reporter=list",
		"headless": "npx playwright test --project=chromium",
		"test:firefox": "npx playwright test --project=firefox --headed",
		"test:webkit": "npx playwright test --project=webkit --headed",
		"test:all": "npx playwright test",
		"test:report": "allure generate playwright-report/allure -o allure-report --clean",
		"allure": "allure open allure-report"
	},
	"keywords": [
		"playwright"
	],
	"author": "upex",
	"license": "ISC",
	"type": "module"

}
