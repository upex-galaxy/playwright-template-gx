{
	"name": "playwright-ts",
	"version": "18",
	"description": "playwright project with typescript and azure devops integration",
	"main": "index.js",
	"scripts": {
		"pw:browsers": "npx playwright install chromium firefox webkit",
		"plan": "npx playwright test --project=chromium --list",
		"test": "npx playwright test --project=chromium",
		"debug": "npx playwright test --project=chromium --debug --reporter=list",
		"test:chrome": "npx playwright test --project=chromium --headed --reporter=list",
		"test:firefox": "npx playwright test --project=firefox --headed --reporter=list",
		"test:webkit": "npx playwright test --project=webkit --headed --reporter=list",
		"regression": "npx playwright test --reporter=list",
		"allure": "allure generate --clean && allure open --host localhost --port 9090"
	},
	"keywords": [
		"playwright"
	],
	"author": "upex",
	"license": "ISC",
	"type": "module",
	"devDependencies": {
		"@playwright/test": "1.30.0",
		"@typescript-eslint/eslint-plugin": "5.53.0",
		"@typescript-eslint/parser": "5.53.0",
		"allure-commandline": "2.21.0",
		"allure-playwright": "2.0.0-beta.26",
		"dotenv": "16.0.3",
		"eslint": "8.34.0",
		"eslint-plugin-only-warn": "1.1.0",
		"prettier": "2.8.4",
		"typescript": "4.9.5"
	}
}
