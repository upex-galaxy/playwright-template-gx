{
	"[Typescript]": {
		"editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
	},
	"[javascript]": {
		"editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
	},
	"files.associations": {
		"*.yml": "yaml"
	},
	"[yaml]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.autoIndent": "keep",
		"diffEditor.ignoreTrimWhitespace": false,
		"editor.defaultFormatter": "kennylong.kubernetes-yaml-formatter",
		"editor.formatOnSave": true
	},
	"cypressHelper.menuItems": {
		"FindCustomCommandReferences": true,
		"FindStepDefinitionReferences": true,
		"GenerateCustomCommandTypes": true,
		"GoToCustomCommand": false,
		"OpenCypress": false,
		"ParseHAR": false,
		"RunCypress": false,
		"SchemaGenerator": false,
		"mockAutocomplete": false
	},
	"editor.autoIndent": "advanced",
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"editor.cursorBlinking": "expand",
	"editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
	"editor.formatOnPaste": false, // required 
	"editor.formatOnType": false, // required
	"editor.formatOnSave": true, // optional 
	"editor.formatOnSaveMode": "file", // required to format on save
	"vs-code-prettier-eslint.prettierLast": false, // set as "true" to run 'prettier' last not first
	"editor.lineHeight": 20,
	"editor.defaultColorDecorators": true,
	"editor.insertSpaces": false,
	"editor.tabSize": 4,
	"editor.detectIndentation": true,
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit"
	},
	"editor.quickSuggestions": {
		"comments": true,
		"other": true,
		"strings": true
	},
	"editor.tabCompletion": "on",
	"eslint.codeAction.showDocumentation": {
		"enable": true
	},
	"eslint.format.enable": true,
	"eslint.validate": ["typescript", "javascript", "ts", "js"],
	"explorer.fileNesting.enabled": true,
	"explorer.compactFolders": false,
	"files.autoSave": "onFocusChange",
	"git.enableSmartCommit": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"material-icon-theme.folders.color": "#7cb342",
	"material-icon-theme.files.associations": {
		"tsconfig.json": "svgo",
		"tsconfig.eslint.json": "eslint",
		"modules.d.ts": "typescript",
		"regression.yml": "test-ts",
		"sanity-template.yml": "test-ts",
		"playwright.chrome.config.ts": "playwright",
		"playwright.firefox.config.ts": "playwright",
		"playwright.edge.config.ts": "playwright",
		"playwright.iphone.config.ts": "playwright",
		"playwright.azure.config.ts": "azure",
		"*.test.js": "test-tsx",
		"testUtils.ts": "3d",
		"TestBase.ts": "3d"
	},
	"material-icon-theme.folders.associations": {
		"workflows": "github",
		"test-html-report": "resolver",
		"test-junit-report": "resolver",
		"test-results": "content",
		"allure-results": "content",
		"allure-report": "mjml",
		"support": "tools"
	},
	"testing.alwaysRevealTestOnStateChange": true,
	"testing.automaticallyOpenPeekViewDuringAutoRun": true,
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"terminal.integrated.tabs.defaultColor": "terminal.ansiGreen",
	"terminal.integrated.tabs.defaultIcon": "source-control",
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"workbench.colorTheme": "Winter is Coming (Dark Blue)",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.settings.editor": "json",
	"workbench.tree.enableStickyScroll": true,
	"[github-actions-workflow]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	}
}